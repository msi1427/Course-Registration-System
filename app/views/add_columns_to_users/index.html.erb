<h1>Add Columns To Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_columns_to_users.each do |add_columns_to_user| %>
      <tr>
        <td><%= link_to 'Show', add_columns_to_user %></td>
